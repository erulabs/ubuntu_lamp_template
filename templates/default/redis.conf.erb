daemonize yes
pidfile /var/run/redis.pid
logfile /var/log/redis.log

port 6379
bind 0.0.0.0
timeout 300

loglevel notice

## Default configuration options
databases 16

# save 900 1
# save 300 1
# save 60 10000

maxmemory <%= @maxmemory %>mb
maxmemory-policy allkeys-lru

rdbcompression yes
dbfilename dump.rdb

appendonly no
