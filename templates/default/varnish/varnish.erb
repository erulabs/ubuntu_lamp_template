START=yes

NFILES=131072

MEMLOCK=82000

DAEMON_OPTS="-a :80 \
             -T localhost:6082 \
             -f /etc/varnish/default.vcl \
             -S /etc/varnish/secret \
             -p thread_pools=4 \
             -p thread_pool_min=50 \
             -p thread_pool_max=1000 \
             -s malloc,<%= @malloc %>m"
